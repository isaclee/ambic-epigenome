#!/bin/bash -l

#SBATCH
#SBATCH --job-name=untar
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=1
#SBATCH --mail-type=end
#SBATCH --mail-user=ilee29@jhmi.edu

if [ -z $1 ]; then
  echo "must supply the file"
else
  infile=`readlink -f $1`
fi

echo "untarring $infile : $SLURM_JOBID"
tar xvzf $infile
echo "Finished with job $SLURM_JOBID"
